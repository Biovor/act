<div class="encre" id="partenaires"></div>
<div class="partners">
    <h2 class="center">Nos Partenaires</h2>
    <div class="row">
        <div class="center center-block partner-row">
            {% for partner in partners %}
                {% for partnerView in partnerViews %}
                    <div class="col s{{ partnerView.s }} m{{ partnerView.m }} l{{ partnerView.l }}">
                        <a href="{{ partner.path }}" target='_blank' rel=noopener >
                            <img class="responsive-img lazyload" src="{{ asset('img/'~partner.picture.pictureName) }}" alt="{{
                            partner.picture.alt }}" >
                        </a>
                    </div>
                {% endfor %}
            {% endfor %}
        </div>
    </div>
</div>

